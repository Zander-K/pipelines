    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pipeline CLI - generate",
            "request": "launch",
            "type": "dart",
            "program": "pipeline_cli/bin/pipe.dart",
            "args": [
                "-g",
                "-b",
                "generate-pipeline",
            ]
        },
        {
            "name": "Pipeline CLI - release",
            "request": "launch",
            "type": "dart",
            "program": "pipeline_cli/bin/pipe.dart",
            "args": [
                "-r",
                "--source-token",
                "token",
                "--notes",
                "./apps/grosvenor_prod/CHANGELOG.txt",
                "--target-repo",
                "rankengineering/sdet_dummy",
                "--no-interactive",
                "--assets",
                "./simple-file.txt",
                "--branch",
                "sdet-pipeline",
                "--environment",
                ""
            ]
        },
        {
            "name": "grosvenor_prod",
            "cwd": "apps/grosvenor_prod",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "grosvenor_prod (profile mode)",
            "cwd": "apps/grosvenor_prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "grosvenor_prod (release mode)",
            "cwd": "apps/grosvenor_prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}